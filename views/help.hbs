                <div>		          
                    <label for="message">
                        <span class="required">Message: *</span> 
                        <textarea id="message" name="msg" placeholder="Please write your message here." tabindex="5" required="required"></textarea> 
                    </label>  
                  </div>



                  <div class="rate">
                    <h7> &nbsp;Rate Us : &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;    </h7>
                    <input name="star" type="radio" id="star5" name="rate" value="5" />
                    <label for="star5" title="text">5 stars</label>
                    <input name="star" type="radio" id="star4" name="rate" value="4" />
                    <label for="star4" title="text">4 stars</label>
