                    <label for="star2" title="text">2 stars</label>
                    <input name="star" type="radio" id="star1" name="rate" value="1" />
                    <label for="star1" title="text">1 star</label>
                  </div><br> <br>




                  <div>		           
                    <button name="submit" type="submit" id="submit" >SEND</button> 
